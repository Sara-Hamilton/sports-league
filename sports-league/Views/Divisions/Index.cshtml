@model IEnumerable<SportsLeague.Models.Division>

@{
    ViewData["Title"] = "Division";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Divisions</h2>
<hr />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>
         @foreach (var listDivision in Model)
  
        {
           <tr>
               <td> @Html.DisplayFor(modelDivision => listDivision.Name)</td>
           
               <td>@Html.ActionLink("Edit", "Edit", new { id = listDivision.DivisionId })</td>
           </tr>

        }
   
 
</table>

@Html.ActionLink("Create new division", "Create")